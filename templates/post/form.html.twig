{% extends 'clean.html.twig' %}

{% block title %}Titre Home
{% endblock %}
{% block stylesheets %}
<style>
header {
	background-image: url('{{ asset("clean/assets/img/home-bg.jpg") }}');
}

</style>
{% endblock %}
{% block header %}
	<!-- Page Header-->
	<header class="masthead">

        <div class="container position-relative px-4 px-lg-5">
            <div class="row gx-4 gx-lg-5 justify-content-center">
                <div class="col-md-10 col-lg-8 col-xl-7">
                    <div class="site-heading">
                        <h1>Formulaire d'ajout d'article</h1>
                        <span class="subheading">Ajout d'article</span>
                    </div>
                </div>
            </div>
        </div>
    </header>
	
{% endblock %}

{% block body %}
	<!-- Main Content-->
	<div class="container px-4 px-lg-5">
		<div class="row gx-4 gx-lg-5 justify-content-center">
		{{ form(form) }}
		</div>
	</div>
{% endblock %}
